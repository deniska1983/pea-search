<h1>Listing dcounts</h1>

<table>
  <tr>
    <th>Ip</th>
  </tr>

<% @dcounts.each do |dcount| %>
  <tr>
    <td><%=h dcount.ip %></td>
    <td><%= link_to 'Show', dcount %></td>
    <td><%= link_to 'Edit', edit_dcount_path(dcount) %></td>
    <td><%= link_to 'Destroy', dcount, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New dcount', new_dcount_path %>