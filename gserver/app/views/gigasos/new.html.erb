<h1>New gigaso</h1>

<% form_for(@gigaso,:html => { :multipart => true}) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :version %><br />
    <%= f.text_field :version %>
  </p>
  <p>
    <%= f.label :desc %><br />
    <%= f.text_area :desc %>
  </p>
  <p>
    <%= f.label :path %><br />
    <%= file_column_field 'gigaso', 'path' %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', gigasos_path %>
